{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#FFEE58",
            "style": "plain",
            "template": "\uf0e7",
            "type": "root"
          },
          {
            "foreground": "#ff4141",
            "style": "plain",
            "template": " \ue20f ",
            "type": "status"
          },
          {
            "background": "#91ddff",
            "foreground": "#193549",
            "style": "powerline",
            "template": " {{ .Name }} ",
            "type": "shell"
          },
          {
            "background": "#d1a5f1",
            "foreground": "#100e23",
            "powerline_symbol": "\ue0b0",
            "style": "powerline",
            "properties": {
              "style": "full"
            },
            "template": " {{ .Path }} ",
            "type": "path"
          },
          {
            "background": "#95ffa4",
            "foreground": "#193549",
            "powerline_symbol": "\ue0b0",
            "style": "powerline",
            "properties": {
              "fetch_upstream_icon": true
            },
            "template": " {{ .UpstreamIcon }} {{ .HEAD }} ",
            "type": "git"
          },
          {
            "background": "#906cff",
            "foreground": "#100e23",
            "powerline_symbol": "\ue0b0",
            "style": "powerline",
            "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
            "type": "python"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
  